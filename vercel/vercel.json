{
  "redirects": [
    {
      "source": "/coverage",
      "destination": "/coverage/",
      "permanent": false
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "buildonchainapps.xyz"
        }
      ],
      "destination": "https://onchainkit.xyz/guides/build-onchain-apps",
      "permanent": false
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "buildonchainapps.xyz"
        }
      ],
      "destination": "https://onchainkit.xyz/guides/build-onchain-apps",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/coverage/",
      "destination": "https://onchainkit-coverage.vercel.app/"
    },
    {
      "source": "/coverage/:match*",
      "destination": "https://onchainkit-coverage.vercel.app/:match*"
    },
    {
      "source": "/playground",
      "destination": "https://onchainkit-bsvq.vercel.app/"
    },
    {
      "source": "/playground/:match*",
      "destination": "https://onchainkit-bsvq.vercel.app/:match*"
    },
    {
      "source": "/templates/onchain-app",
      "destination": "https://onchain-app-template.vercel.app/"
    },
    {
      "source": "/templates/onchain-app/:match*",
      "destination": "https://onchain-app-template.vercel.app/:match*"
    },
    {
      "source": "/",
      "destination": "https://onchainkit-coinbase-vercel.vercel.app/"
    },
    {
      "source": "/:match*",
      "destination": "https://onchainkit-coinbase-vercel.vercel.app/:match*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "x-robots-tag",
          "value": "index, follow"
        }
      ]
    }
  ]
}
